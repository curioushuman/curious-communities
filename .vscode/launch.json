{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug:nx-test:cc-api-admin",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnFirstSessionStart",
      // "preLaunchTask": "cdk-debug:on",
      // "postDebugTask": "cdk-debug:off",
      "program": "${workspaceFolder}/node_modules/nx/bin/nx.js",
      "args": ["test", "cc-api-admin", "--codeCoverage=false"],
      "env": {
        "CDK_DEBUG": "true"
      }
    },
    {
      "name": "debug:nx-test-infra:cc-api-admin",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnFirstSessionStart",
      // "preLaunchTask": "cdk-debug:on",
      // "postDebugTask": "cdk-debug:off",
      "program": "${workspaceFolder}/node_modules/nx/bin/nx.js",
      "args": ["test-infra", "cc-api-admin", "--codeCoverage=false"],
      "env": {
        "CDK_DEBUG": "true"
      }
    }
  ]
}
