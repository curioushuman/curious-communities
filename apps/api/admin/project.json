{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/api/admin/**/*.ts"]
      }
    },
    "list": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "list",
        "parameters": [
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"]
    },
    "build": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "synth",
        "parameters": [
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      },
      {
        "target": "openapi",
        "projects": "self"
      }]
    },
    "synth": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "synth",
        "parameters": [
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      },
      {
        "target": "openapi",
        "projects": "self"
      }]
    },
    "deploy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "parameters": [
          "--all",
          " --require-approval=never",
          "--outputs-file ../../../dist/apps/api/admin/cdk-outputs.json",
          "--profile=$AWS_PROFILE"
        ],
        "outputPath": "dist/apps/api/admin",
        "command": "deploy"
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "deploy",
        "projects": "dependencies"
      },
      {
        "target": "openapi",
        "projects": "self"
      }]
    },
    "destroy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "parameters": [
          "--all",
          "--force",
          "--profile=$AWS_PROFILE"
        ],
        "outputPath": "dist/apps/api/admin",
        "command": "destroy"
      }
    },
    "diff": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "diff",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      },
      {
        "target": "openapi",
        "projects": "self"
      }]
    },
    "metadata": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "metadata",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "context": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "context",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "docs": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "docs",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "dependsOn": [{
        "target": "openapi",
        "projects": "self"
      }]
    },
    "doctor": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/api/admin",
        "command": "doctor",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/api/admin"],
      "options": {
        "jestConfig": "apps/api/admin/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "test-infra": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/api/admin/jest.config.infra.ts",
        "passWithNoTests": true,
        "cwd": "apps/api/admin"
      }
    },
    "openapi": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "../../../node_modules/.bin/swagger-cli bundle openapi.yaml --outfile ../../../dist/apps/api/admin/openapi.yaml --type yaml"
          }
        ],
        "cwd": "apps/api/admin"
      }
    },
    "deploy-local": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "../../../node_modules/.bin/rimraf ../../../dist/apps/api/admin",
          "../../../node_modules/.bin/cdklocal deploy --all --require-approval=never --output ../../../dist/apps/api/admin --outputs-file ../../../dist/apps/api/admin/cdk-outputs.json"
        ],
        "cwd": "apps/api/admin",
        "parallel": false
      }
    },
    "destroy-local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../../node_modules/.bin/cdklocal destroy --all --force --output ../../../dist/apps/api/admin",
        "cwd": "apps/api/admin",
        "parallel": false
      }
    }
  },
  "tags": []
}
