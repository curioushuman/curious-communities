{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/core/groups/**/*.ts"]
      }
    },
    "list": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "list",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"]
    },
    "build": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "synth",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": []
    },
    "synth": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "synth",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": []
    },
    "deploy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "deploy",
        "parameters": [
          "--all",
          " --require-approval=never",
          "--outputs-file ../../../dist/apps/core/groups/cdk-outputs.json",
          "--profile=$AWS_PROFILE"
        ]
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": []
    },
    "destroy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "destroy",
        "parameters": [
          "--all",
          "--force",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "diff": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "diff",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      },
      "dependsOn": []
    },
    "metadata": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "metadata",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "context": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "context",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "docs": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "docs",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "doctor": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/core/groups",
        "command": "doctor",
        "parameters": [
          "--all",
          "--profile=$AWS_PROFILE"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/core/groups"],
      "options": {
        "jestConfig": "apps/core/groups/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy-local": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "../../../node_modules/.bin/rimraf ../../../dist/apps/core/groups",
          "../../../node_modules/.bin/cdklocal deploy --require-approval=never --output ../../../dist/apps/core/groups"
        ],
        "cwd": "apps/core/groups",
        "parallel": false
      }
    },
    "destroy-local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../../node_modules/.bin/cdklocal destroy --force --output ../../../dist/apps/core/groups",
        "cwd": "apps/core/groups",
        "parallel": false
      }
    }
  },
  "tags": []
}
